 )
 @settings(max_examples=200)
+@example(
+    # The test always failed when commented parts were varied together.
+    cfg={
+        "language": "en-GB",
+        "latex_engine": "lualatex",
+        "math": {
+            "expect_brackets": True,
+            "prob_brackets": True,
+            "scriptstyle_depth": 2,
+            "raisebox_ex": 0.2,
+        },
+        "environments": {
+            "forbidden": ["eqnarray", "array"],
+            "lists_enumitem_only": True,
+            "list_opts": "noitemsep, topsep=0pt",
+        },
+        "theorems": {"label_prefixes": ["thm", "lemma", "prop", "cor", "defn", "ex"]},
+        "punctuation": {
+            "abbreviations": ["w.l.o.g.", "a.s.", "i.i.d.", "w.r.t.", "a.e."],
