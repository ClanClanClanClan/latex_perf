+        },
+        "orthography": {
+            "curly_quotes": True,
+            "en_dash_ranges": True,
+            "enforce_nfc": True,
+        },
+        "labels": {"unique": True, "all_referenced": True},
+        "bibliography": {"require_doi": True, "require_url": True},
+        "compilation": {"zero_warnings": True},
+        "logging": {"report": "html"},
+    },
+    arg="x",  # or any other generated value
+    with_label=False,  # or any other generated value
